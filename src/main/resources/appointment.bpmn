      <bpmn:outgoing>SequenceFlow_1n9ielp</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="SequenceFlow_1n9ielp" sourceRef="Task_047qo9j" targetRef="IntermediateThrowEvent_08koyuz"/>
